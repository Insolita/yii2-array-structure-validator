{
    "name": "insolita/yii2-array-structure-validator",
    "description": "Validate array with complex structure",
    "type": "library",
    "keywords": [
        "yii2",
        "validator"
    ],
    "require": {
        "php":            "^7.1|^8.0",
        "yiisoft/yii2":   "~2.0.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "cebe/indent": "*"
    },
    "autoload": {
        "psr-4": {
            "insolita\\ArrayStructureValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "insolita",
            "email": "webmaster100500@ya.ru"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
